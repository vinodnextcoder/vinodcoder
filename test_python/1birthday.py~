import time
import calendar
import re
f=open("input_birthday.txt","r")
r=f.read()
f.close()
r=r.replace("\r","")
r=r.strip()
data=r.split("\n")


dict_bdate={}
for month in range(04,05):
    c = calendar.monthcalendar(2018, month)
    first_week = c[0]
    second_week = c[1]
    third_week = c[2]
    fourth_week = c[3]
    fifth_week=c[4]
date=[]
for x in data:
    name=re.split(" ",x)
    #print name
    dict_bdate[name[0]]=name[1]
    bdate=re.split("-",name[0])
    date.append(int(bdate[2]))
# print date
#print dict_bdate
weekindex = []
for i in range(0,len(c)):
	#print c[i]
	for d in date:
		if d in c[i]:
                    #print d,i
                    weekindex.append(i)
